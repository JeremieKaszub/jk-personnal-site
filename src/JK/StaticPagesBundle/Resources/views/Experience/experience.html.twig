		{% include "JKStaticPagesBundle:Experience:_proxp.html.twig" %}
	{% endif %}
	{% if path(app.request.attributes.get('_route'), app.request.attributes.get('_route_params')) == path(app.request.attributes.get('_route')) ~ '/school' %}
